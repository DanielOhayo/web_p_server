{
    "version": 2,
    "name": "GradeHub-server",
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)", 
            "dest": "index.js"
        }
    ],
    "static": [
        "public"
    ]
}